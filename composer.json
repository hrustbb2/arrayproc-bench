{
  "autoload": {
    "psr-4": {
      "Lib\\": "lib/",
      "Bench\\": "bench/",
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "bench": [
      "vendor/bin/phpbench run bench/Bench.php --report=default"
    ],
    "test": [
      "vendor/bin/phpunit tests/Test.php"
    ]
  },
  "require": {
    "laravel/framework": "^5.6",
    "phpbench/phpbench": "^0.17",
    "phpunit/phpunit": "^9.0",
    "hrustbb2/arrayproc": "dev-master"
  }
}
